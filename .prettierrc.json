{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "semi": true,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "proseWrap": "always",
  "importOrder": ["^@angular/", "^[^.]"],
  "importSortOrder": ["asc", "asc"],
  "plugins": ["prettier-plugin-tailwindcss"]
}
